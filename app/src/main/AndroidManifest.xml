<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.playup">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".dagger.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/iconapplight"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/iconapplight"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.DayNight.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".chat.ChatActivity"></activity>
        <activity android:name=".menu.MenuActivity" />
        <activity android:name=".user.UserActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>